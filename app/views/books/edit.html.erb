<div class="edit-book">
		<h1>Editing Book</h1>
	<%= form_for(@bookf) do |f| %>

	    <h4>title</h4>
	    <%= f.text_field :title %>

	    <h4>opinion</h4>
	    <%= f.text_area :body %>
